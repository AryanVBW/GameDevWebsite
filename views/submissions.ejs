<table>
  <thead>
    <tr>
      <th>Team Name</th>
      <th>URL</th>
      <th>Game Description</th>
      <th>gameGenre</th>
    </tr>
  </thead>
  <tbody>
    <% if (submissions.length === 0) { %>
    <tr>
      <td colspan="4">No submissions yet!</td>
    </tr>
    <% } else { %> <% submissions.forEach((submission) => { %>
    <tr class="<%= (submission.name == teamName) ? 'active' : '' %>">
      <td><%= submission.name %></td>
      <td><%= submission.itchioURL %></td>
      <td><%= submission.gameDescription %></td>
      <td><%= submission.gameGenre %></td>
    </tr>
    <% }) } %>
  </tbody>
</table>
